public string Execute(Tipo tipo)
        {
            HttpWebRequest request = (HttpWebRequest)WebRequest.Create("http://ponto.db1.com.br/ponto/pages/login.jsf");

            request.Method = "POST";

            var formContent =
                string.Format("{0}={1}", HttpUtility.UrlEncode("login:login"), HttpUtility.UrlEncode("MARCOS.TOMAZINI")) +
                string.Format("&{0}={1}", HttpUtility.UrlEncode("login:senha"), HttpUtility.UrlEncode("SENHAA")) +
                string.Format("&{0}={1}", HttpUtility.UrlEncode("login:tipo"), HttpUtility.UrlEncode(tipo == Tipo.Entrada ? "E" : "S")) +

                string.Format("&{0}={1}", HttpUtility.UrlEncode("login:_id15"), HttpUtility.UrlEncode("LOGIN")) +
                //string.Format("&{0}={1}", HttpUtility.UrlEncode("login:_id17"), HttpUtility.UrlEncode("RELATÓRIO")) +
                //string.Format("&{0}={1}", HttpUtility.UrlEncode("login:_id19"), HttpUtility.UrlEncode("CONFERÊNCIA TASK E PONTO")) +

                string.Format("&{0}={1}", HttpUtility.UrlEncode("login_SUBMIT"), HttpUtility.UrlEncode("1")) +
                string.Format("&{0}={1}", HttpUtility.UrlEncode("login:_idcl"), HttpUtility.UrlEncode("")) +
                string.Format("&{0}={1}", HttpUtility.UrlEncode("login:_link_hidden_"), HttpUtility.UrlEncode("")) +
                string.Format("&{0}={1}", HttpUtility.UrlEncode("javax.faces.ViewState"), HttpUtility.UrlEncode(""));


            byte[] byteArray = Encoding.UTF8.GetBytes(formContent);
            request.ContentType = "application/x-www-form-urlencoded";
            request.ContentLength = byteArray.Length;
            Stream dataStream = request.GetRequestStream();
            dataStream.Write(byteArray, 0, byteArray.Length);
            dataStream.Close();

            WebResponse response = request.GetResponse();
            dataStream = response.GetResponseStream();
            StreamReader reader = new StreamReader(dataStream);
            var result = reader.ReadToEnd();
            //string responseFromServer = HttpUtility.UrlDecode(reader.ReadToEnd());
            //You may need HttpUtility.HtmlDecode depending on the response

            reader.Close();
            dataStream.Close();
            response.Close();

            return result;
        }